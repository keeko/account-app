{
	"name" : "keeko/account-app",
	"description" : "Keeko Account Application",
	"type" : "keeko-app",
	"license" : "MIT",
	"authors" : [{
			"name" : "gossi"
		}
	],
	"autoload" : {
		"psr-4" : {
			"keeko\\account-app\\" : "src/"
		}
	},
	"require" : {
		"php" : ">=5.4",
		"keeko/composer-installer" : "*",
		"fxp/composer-asset-plugin" : "~1.0",
		"puli/repository" : "^1.0@beta",
		"bower-asset/bootstrap" : "~3",
		"bower-asset/font-awesome" : "~4"
	},
	"require-dev" : {
		"keeko/framework" : "dev-master",
		"keeko/core" : "@dev",
		"composer/composer" : "@dev",
		"propel/propel" : "@dev",
		"puli/composer-plugin" : "@beta",
		"puli/twig-extension" : "@beta",
		"puli/url-generator" : "@beta",
		"puli/discovery" : "@beta"
	},
	"extra" : {
		"keeko" : {
			"app" : {
				"title" : "Keeko Account Application",
				"class" : "keeko\\application\\account\\AccountApplication"
			}
		}
	}
}